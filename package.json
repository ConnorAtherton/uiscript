{
  "name": "uiscript",
  "version": "0.1.0",
  "description": "A human-readable web ui manipulation language",
  "main": "dist/uiscript.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "build": "babel src --out-dir dist/ && mv -vf dist/cli.js bin/uiscript",
    "watch": "babel src --out-dir dist/ -w && mv -vf dist/cli.js bin/uiscript",
    "prepublish": "npm run build",
    "lint": "eslint src/ test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ConnorAtherton/uiscript.git"
  },
  "keywords": [
    "ui",
    "script",
    "animation"
  ],
  "bin": "bin/uiscript",
  "author": "Connor Atherton <cliam.atherton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ConnorAtherton/uiscript/issues"
  },
  "homepage": "https://github.com/ConnorAtherton/uiscript#readme",
  "dependencies": {
    "commander": "2.9.0",
    "concat-stream": "1.5.1",
    "eslint": "^2.3.0",
    "glob": "6.0.4",
    "headway": "0.3.0",
    "minimist": "^1.2.0",
    "split": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.3.13",
    "debug": "^2.2.0",
    "mocha": "^2.4"
  },
  "files": [
    "bin",
    "dist",
    "LICENSE",
    "README.md"
  ]
}
